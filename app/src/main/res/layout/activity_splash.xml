<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mobile.paolo.listaspesa.view.init.SplashActivity"
    android:id="@+id/splashView">

    <ImageView
        android:id="@+id/logo_white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/ic_shopping_cart_white_64dp"
        android:transitionName="logo_shared"/>

    <ImageView
        android:id="@+id/logo_blue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/ic_shopping_cart_blue_64dp"
        android:transitionName="logo_shared"/>



</android.widget.RelativeLayout>
